{
  "manifest_version": 2,
  "name": "AutoAuth",
  "version": "3.1.1",
  "description": "Automatically provides HTTP authentication credentials.",
  "permissions": [
      "webRequest",
      "webRequestBlocking",
      "storage",
      "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./autologin.js"]
    }
  ],
  "options_ui": {
      "page": "options.html",
      "browser_style": true
  },
  "applications": {
    "gecko": {
      "id": "autoauth@trustingwolves.com",
      "strict_min_version": "57.0"
    }
  }
}
